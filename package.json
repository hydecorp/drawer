{
  "name": "hy-drawer",
  "version": "1.0.0-stencil",
  "description": "A touch-enabled drawer component for the modern web",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist www",
    "test": "exit 0",
    "build": "stencil build --docs",
    "watch": "stencil build --dev --watch --serve",
    "preversion": "sed -i '' 's:/dist:#/dist:' .gitignore",
    "version": "npm run build && git add .",
    "postversion": "sed -i '' 's:#/dist:/dist:' .gitignore && git rm --cached -r dist && git add . && git commit -m 'restore preversion .gitignore'"
  },
  "config": {
    "port": "8082"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "GPL-3.0",
  "devDependencies": {
    "@stencil/core": "^0.18.1",
    "@stencil/sass": "^0.1.1"
  },
  "dependencies": {
    "rxjs": "^6.4.0",
    "rxjs-create-tween": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwtel/hy-drawer.git"
  },
  "bugs": {
    "url": "https://github.com/qwtel/hy-drawer/issues"
  },
  "homepage": "https://qwtel.com/hy-drawer/"
}
